{
    "name": "Puppeteer Extension Example v3",
    "version": "0.0.0.1",
    "description": "Example extension which uses puppeteer-extension-transport",
    "action": {
        "default_title": "Click to Debug!!!"
    },
    "permissions": [
        "tabs",
        "debugger",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "manifest_version": 3
}